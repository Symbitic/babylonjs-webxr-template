{
  "name": "babylonjs-webxr-template",
  "description": "Babylon.js template using WebXR",
  "version": "1.0.0",
  "author": "Alex Shaw <alex.shaw.as@gmail.com>",
  "private": true,
  "license": "CC0-1.0",
  "repository": "https://github.com/Symbitic/babylonjs-webxr-template",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --env production",
    "lint": "eslint \"./src/**/*.{js,ts}\"",
    "preserve": "npm run build",
    "pretest": "npm run build",
    "test": "npm run test",
    "test:run": "node test/test.mjs",
    "types:check": "tsc --noEmit",
    "serve": "serve -p 8080 dist",
    "start": "cross-env NODE_ENV=development webpack serve --env development"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@babylonjs/core": "^4.2.0",
    "@babylonjs/gui": "^4.2.0",
    "@babylonjs/inspector": "^4.2.0",
    "@babylonjs/loaders": "^4.2.0",
    "cannon": "^0.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.14.0",
    "@types/cannon": "^0.1.5",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "babel-loader": "^8.2.2",
    "chalk": "^4.1.1",
    "compression-webpack-plugin": "^8.0.0",
    "copy-webpack-plugin": "^9.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.27.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "file-loader": "^6.2.0",
    "gh-pages": "^3.2.0",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "pixelmatch": "^5.2.1",
    "pngjs": "^6.0.0",
    "puppeteer": "^10.0.0",
    "serve": "^12.0.0",
    "serve-handler": "^6.1.3",
    "typescript": "^4.3.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  }
}
